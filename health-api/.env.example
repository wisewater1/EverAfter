# Server Configuration
NODE_ENV=development
PORT=4000
BASE_URL=http://localhost:4000

# Database (Supabase PostgreSQL)
DATABASE_URL=postgresql://postgres:[PASSWORD]@[HOST]:5432/postgres
SUPABASE_URL=https://[PROJECT-REF].supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Redis (for BullMQ job queue)
REDIS_URL=redis://localhost:6379

# JWT Authentication
JWT_SECRET=your_jwt_secret_key_here

# Terra API (Multi-Device Aggregator - 300+ devices)
TERRA_API_KEY=your_terra_api_key
TERRA_DEV_ID=your_terra_dev_id
TERRA_WEBHOOK_SECRET=your_terra_webhook_secret
TERRA_CLIENT_ID=your_terra_client_id
TERRA_CLIENT_SECRET=your_terra_client_secret

# Fitbit API
FITBIT_CLIENT_ID=your_fitbit_client_id
FITBIT_CLIENT_SECRET=your_fitbit_client_secret
FITBIT_SUBSCRIBER_VERIFICATION_CODE=your_fitbit_subscriber_code

# Oura Ring API
OURA_CLIENT_ID=your_oura_client_id
OURA_CLIENT_SECRET=your_oura_client_secret

# Dexcom CGM API
DEXCOM_CLIENT_ID=your_dexcom_client_id
DEXCOM_CLIENT_SECRET=your_dexcom_client_secret

# Strava API
STRAVA_CLIENT_ID=your_strava_client_id
STRAVA_CLIENT_SECRET=your_strava_client_secret

# Whoop API
WHOOP_CLIENT_ID=your_whoop_client_id
WHOOP_CLIENT_SECRET=your_whoop_client_secret

# Garmin Health API (requires partnership)
GARMIN_CONSUMER_KEY=your_garmin_consumer_key
GARMIN_CONSUMER_SECRET=your_garmin_consumer_secret

# Withings API
WITHINGS_CLIENT_ID=your_withings_client_id
WITHINGS_CLIENT_SECRET=your_withings_client_secret

# Polar AccessLink API
POLAR_CLIENT_ID=your_polar_client_id
POLAR_CLIENT_SECRET=your_polar_client_secret

# Google Fit API
GOOGLE_FIT_CLIENT_ID=your_google_fit_client_id
GOOGLE_FIT_CLIENT_SECRET=your_google_fit_client_secret

# Abbott LibreView API
ABBOTT_CLIENT_ID=your_abbott_client_id
ABBOTT_CLIENT_SECRET=your_abbott_client_secret

# Mobile Bridge Authentication
APPLE_HEALTH_BRIDGE_SECRET=your_apple_bridge_shared_secret
HEALTH_CONNECT_BRIDGE_SECRET=your_android_bridge_shared_secret

# Development/Testing
MOCK_PROVIDERS=false
ENABLE_SWAGGER_UI=true
LOG_LEVEL=info

# Webhook Configuration
WEBHOOK_TIMEOUT_MS=30000
WEBHOOK_RETRY_ATTEMPTS=3
WEBHOOK_RETRY_DELAY_MS=5000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
